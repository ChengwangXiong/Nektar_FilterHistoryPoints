<?xml version="1.0" encoding="utf-8" ?>
<NEKTAR>
    <GEOMETRY DIM="2" SPACE="2">
        <VERTEX>
            <V ID="0">1.000e+00 1.000e+00 0.000e+00</V>
            <V ID="1">-1.000e+00 1.000e+00 0.000e+00</V>
            <V ID="2">-1.000e+00 -1.000e+00 0.000e+00</V>
            <V ID="3">1.000e+00 -1.000e+00 0.000e+00</V>
        </VERTEX>
        <EDGE>
            <E ID="0">    0  1   </E>
            <E ID="1">    1  2   </E>
            <E ID="2">    2  3   </E>
            <E ID="3">    3  0   </E>
        </EDGE>
        <ELEMENT>
            <Q ID="0">    0     1     2     3 </Q>
        </ELEMENT>
        <COMPOSITE>
            <C ID="0"> Q[0] </C>
            <C ID="1"> E[2] </C>
            <C ID="2"> E[3] </C>
            <C ID="3"> E[0] </C>
            <C ID="4"> E[1] </C>
        </COMPOSITE>
        <DOMAIN> C[0] </DOMAIN>
    </GEOMETRY>
    <EXPANSIONS>
        <E COMPOSITE="C[0]" NUMMODES="#NUMMODES#" TYPE="MODIFIED" />
    </EXPANSIONS>
    
    <CONDITIONS>

    <PARAMETERS>
      <P> FinTime            = 1.0              </P>
      <P> NumSteps           = #DT#             </P>
      <P> TimeStep           = FinTime/NumSteps </P>
      <P> IO_CheckSteps      = NumSteps         </P>
      <P> IO_InfoSteps       = 1000             </P>
      <P> CFL                = #CFL#            </P>
      <P> NumQuadPointsError = #NQP#           </P>
    </PARAMETERS>

    <SOLVERINFO>
      <I PROPERTY="EQTYPE" VALUE="CFLtester"/>
      <I PROPERTY="Projection" VALUE="DisContinuous"/>    
      <I PROPERTY="TimeIntegrationMethod"  VALUE="#TimeScheme#"/>
    </SOLVERINFO> 

    <VARIABLES>
      <V ID="0"> u </V> 
    </VARIABLES>

    <BOUNDARYREGIONS>
      <B ID="0"> C[1] </B>
      <B ID="1"> C[2] </B>
      <B ID="2"> C[3] </B>
      <B ID="3"> C[4] </B>
    </BOUNDARYREGIONS>

    <BOUNDARYCONDITIONS>
      <REGION REF="0">
        <D VAR="u" USERDEFINEDTYPE="TimeDependent" VALUE="exp(-41*((x+(0.3*cos(2*PI*t)))*(x+(0.3*cos(2*PI*t)))+(y-(0.3*sin(2*PI*t)))*(y-(0.3*sin(2*PI*t)))))" />
      </REGION>
      <REGION REF="1">
        <D VAR="u"  USERDEFINEDTYPE="TimeDependent" VALUE="exp(-41*((x+(0.3*cos(2*PI*t)))*(x+(0.3*cos(2*PI*t)))+(y-(0.3*sin(2*PI*t)))*(y-(0.3*sin(2*PI*t)))))" />
      </REGION>
      <REGION REF="2">
        <D VAR="u"  USERDEFINEDTYPE="TimeDependent" VALUE="exp(-41*((x+(0.3*cos(2*PI*t)))*(x+(0.3*cos(2*PI*t)))+(y-(0.3*sin(2*PI*t)))*(y-(0.3*sin(2*PI*t)))))" />
      </REGION>
      <REGION REF="3">
        <D VAR="u"  USERDEFINEDTYPE="TimeDependent" VALUE="exp(-41*((x+(0.3*cos(2*PI*t)))*(x+(0.3*cos(2*PI*t)))+(y-(0.3*sin(2*PI*t)))*(y-(0.3*sin(2*PI*t)))))" />
      </REGION>
    </BOUNDARYCONDITIONS>

    <USERDEFINEDEQNS>
      <F LHS="Vx" VALUE="2*PI*y"  />
      <F LHS="Vy" VALUE="-2*PI*x" />
    </USERDEFINEDEQNS>   
    
    <EXACTSOLUTION>
      <F VAR="u" VALUE="exp(-41*((x+(0.3*cos(2*PI*t)))*(x+(0.3*cos(2*PI*t)))+(y-(0.3*sin(2*PI*t)))*(y-(0.3*sin(2*PI*t)))))" />
    </EXACTSOLUTION>
    
    <INITIALCONDITIONS>
      <F VAR="u" VALUE="exp(-41*(((x+0.3)*(x+0.3))+((y)*(y))))" />
    </INITIALCONDITIONS>
    
  </CONDITIONS>
  
  <GLOBALOPTIMIZATIONPARAMETERS>
    <SOURCE FILE="tmp_opt.xml" />
  </GLOBALOPTIMIZATIONPARAMETERS>
  
  <ELEMENTALOPTIMIZATIONPARAMETERS>
    <SOURCE FILE="tmp_opt.xml" />
  </ELEMENTALOPTIMIZATIONPARAMETERS>
</NEKTAR>
