    <EXPANSIONS>
       <E COMPOSITE="C[0]" NUMMODES="8" TYPE="MODIFIED" />
    </EXPANSIONS>
    
    <CONDITIONS>
    
       <SOLVERINFO>
        <I PROPERTY="EQTYPE" VALUE="UnsteadyAdvectionDiffusion" />
        <I PROPERTY="Projection" VALUE="Continuous"/>
        <I PROPERTY="DiffusionAdvancement" VALUE="Implicit"/>
        <I PROPERTY="AdvectionAdvancement" VALUE="Explicit"/>
        <I PROPERTY="TimeIntegrationMethod" VALUE="IMEXOrder2"/>
      </SOLVERINFO>

    <PARAMETERS>
      <P> TimeStep      = 0.00001               </P>
      <P> NumSteps      = 2000                 </P>
      <P> IO_CheckSteps = 2000                 </P>
      <P> IO_InfoSteps  = 2000                 </P>
      <P> wavefreq = PI                        </P>
      <P> epsilon = 1.0                        </P>
    </PARAMETERS>
        
    <VARIABLES>
      <V ID="0"> u </V> 
    </VARIABLES>
    
    <BOUNDARYREGIONS>
      <B ID="0"> C[1] </B>
    </BOUNDARYREGIONS>
    
    <BOUNDARYCONDITIONS>
         <REGION REF="0">
            <D VAR="u" USERDEFINEDTYPE="TimeDependent" VALUE="exp(-2.0*wavefreq*wavefreq*t)*sin(wavefreq*x)*cos(wavefreq*y)" />
        </REGION>
    </BOUNDARYCONDITIONS>
    
     <USERDEFINEDEQNS>
       <F LHS="Vx" VALUE="0.0"  />
       <F LHS="Vy" VALUE="0.0" />
    </USERDEFINEDEQNS>
    
    <INITIALCONDITIONS>
        <F VAR="u" VALUE="sin(wavefreq*x)*cos(wavefreq*y)" />
    </INITIALCONDITIONS>
   
    <EXACTSOLUTION>
      <F VAR="u" VALUE="exp(-2.0*wavefreq*wavefreq*t)*sin(wavefreq*x)*cos(wavefreq*y)" />
    </EXACTSOLUTION>

</CONDITIONS>

</NEKTAR>
